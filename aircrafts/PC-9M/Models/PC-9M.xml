  <object-name>farch</object-name>
  <condition>
   <property>/sim/model/pc9m/t-6</property>
  </condition>
 </animation>

 <effect>
  <name>Effects/pc9-reflection</name>
  <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
  <object-name>arch</object-name>
  <object-name>fuselage</object-name>
  <object-name>wings</object-name>
  <parameters>
    <texture n="2">
      <image>Aircraft/PC-9M/Models/Effects/pc9-normalmap.png</image>
    </texture>
  </parameters>
 </effect>

