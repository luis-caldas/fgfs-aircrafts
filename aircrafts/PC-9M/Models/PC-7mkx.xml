   <y-m> 1.002</y-m>
   <z-m> 0.215</z-m>
  </offsets>
 </model>

 <model>
  <name>Cockpit</name>
  <usage>interior</usage>
  <path>Aircraft/PC-9M/Models/Cockpit/Cockpit-PC7mk.xml</path>
