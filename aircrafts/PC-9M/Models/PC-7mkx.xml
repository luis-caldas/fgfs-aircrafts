   <x-m>-0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m>-0.02</z-m>
  </offsets>
 </model>

 <effect>
  <name>Effects/pc7mk-reflection</name>
  <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
  <object-name>arch</object-name>
  <object-name>fuselage</object-name>
  <object-name>wings</object-name>
  <parameters>
   <texture n="2">
    <image>Aircraft/PC-9M/Models/Effects/pc7mk-normalmap.png</image>
   </texture>
  </parameters>
 </effect>

 <effect>
  <name>Effects/pc9-reflection</name>
  <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
  <object-name>cockpit</object-name>
  <object-name>strut</object-name>
 </effect>

  <!-- glass effects -->
 <effect>
  <inherits-from>Aircraft/PC-9M/Models/Effects/glass/glass</inherits-from>
  <object-name>canopy_inner</object-name>
 </effect>

 <effect>
  <inherits-from>Aircraft/PC-9M/Models/Effects/glass/reflection</inherits-from>
  <object-name>canopy_outer</object-name>
 </effect>

 <animation>
  <type>noshadow</type>
  <object-name>canopy</object-name>
 </animation>

 <effect>
  <inherits-from>Effects/model-transparent</inherits-from>
  <object-name>canopy</object-name>
 </effect>

