<?xml version="1.0"?>

<PropertyList>

 <path>strut_left.ac</path>

 <effect>
  <name>Effects/reflection</name>
  <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
  <object-name>strut</object-name>
 </effect>

 <!-- Right Strut -->
 <animation>
  <type>rotate</type>
  <object-name>strut</object-name>
  <property>gear/gear[2]/position-norm</property>
  <factor>90</factor>
  <offset-deg>-90</offset-deg>
  <center>
   <x1-m> 0.0</x1-m>
   <y1-m> 0.0  </y1-m>
   <z1-m> 0.0</z1-m>
  </center>
  <axis>
   <x>-1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

  <animation>
  <type>rotate</type>
  <object-name>hydro</object-name>
  <property>gear/gear[2]/position-norm</property>
  <factor>-131</factor>
  <offset-deg>131</offset-deg>
  <center>
   <x-m> 0.0  </x-m>
   <y-m> 0.023</y-m>
   <z-m>-0.277</z-m>
  </center>
  <axis>
   <x>-1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>uscissor</object-name>
  <object-name>dscissor</object-name>
  <object-name>compressable</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <factor>30</factor>
  <center>
   <x-m>-0.060</x-m>
   <y-m> 0.0  </y-m>
   <z-m>-0.452</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>dscissor</object-name>
  <object-name>compressable</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <factor>-60</factor>
  <center>
   <x-m>-0.173</x-m>
   <y-m> 0.0  </y-m>
   <z-m>-0.551</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>compressable</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <factor>30</factor>
  <center>
   <x-m>-0.069</x-m>
   <y-m> 0.0  </y-m>
   <z-m>-0.651</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
