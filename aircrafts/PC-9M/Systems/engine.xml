<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <filter>
    <name>Engine RPM</name>
    <type>gain</type>
    <input>
      <property>engines/engine[0]/n1</property>
    </input>
    <output>
      <property>/engines/active-engine/n1</property>
    </output>
  </filter>

  <filter>
    <name>Engine Thrust-Coefficient</name>
    <type>gain</type>
    <input>
      <property>/fdm/jsbsim/propulsion/engine[0]/thrust-coefficient</property>
    </input>
    <output>
      <property>/engines/active-engine/thrust-coefficient</property>
    </output>
  </filter>

  <filter>
    <name>Prop Induced Velocity</name>
    <type>gain</type>
    <input>
      <property>/fdm/jsbsim/propulsion/engine[0]/prop-induced-velocity_fps</property>
    </input>
    <output>
      <property>/engines/active-engine/prop-induced-velocity_fps</property>
    </output>
  </filter>

</PropertyList>
