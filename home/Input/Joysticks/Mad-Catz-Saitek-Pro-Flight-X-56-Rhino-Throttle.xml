<?xml version="1.0"?>

<PropertyList>
  <name type="string">Mad Catz Saitek Pro Flight X-56 Rhino Throttle</name>
  <button n="5">
    <desc type="string">Flaps Up</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.flapsDown(-1);</script>
    </binding>
    <repeatable type="double">0</repeatable>
  </button>
  <button n="6">
    <desc type="string">Flaps Down</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.flapsDown(1);</script>
    </binding>
    <repeatable type="double">0</repeatable>
  </button>
  <button n="7">
    <desc type="string">Gear Up</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.gearDown(-1);</script>
    </binding>
    <repeatable type="double">0</repeatable>
  </button>
  <button n="8">
    <desc type="string">Gear Down</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.gearDown(1);</script>
    </binding>
    <repeatable type="double">0</repeatable>
  </button>
  <button n="9">
    <desc type="string">FGCom PTT</desc>
    <repeatable type="string">false</repeatable>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.ptt(1);</script>
    </binding>
    <mod-up>
      <binding>
        <command type="string">nasal</command>
        <script type="string">controls.ptt(0);</script>
      </binding>
    </mod-up>
  </button>
  <button n="10">
    <desc type="string">FGCom PTT(2)</desc>
    <repeatable type="string">false</repeatable>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.ptt(2);</script>
    </binding>
    <mod-up>
      <binding>
        <command type="string">nasal</command>
        <script type="string">controls.ptt(0);</script>
      </binding>
    </mod-up>
  </button>
  <button n="11">
    <desc type="string">Brakes (air/wheel)</desc>
    <repeatable type="string">false</repeatable>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.applyApplicableBrakes(1);</script>
    </binding>
    <mod-up>
      <binding>
        <command type="string">nasal</command>
        <script type="string">controls.applyApplicableBrakes(0);</script>
      </binding>
    </mod-up>
  </button>
  <button n="12">
    <desc type="string">Parking brakes</desc>
    <repeatable type="string">false</repeatable>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.parkingBrakeToggle(0);</script>
    </binding>
    <mod-up>
      <binding>
        <command type="string">nasal</command>
        <script type="string">controls.parkingBrakeToggle(1);</script>
      </binding>
    </mod-up>
  </button>
  <button n="13">
    <desc type="string">Brakes</desc>
    <repeatable type="string">false</repeatable>
    <binding>
      <command type="string">nasal</command>
      <script type="string">controls.applyBrakes(1);</script>
    </binding>
    <mod-up>
      <binding>
        <command type="string">nasal</command>
        <script type="string">controls.applyBrakes(0);</script>
      </binding>
    </mod-up>
  </button>
  <button n="14">
    <desc>Landing Lights</desc>
    <binding>
      <command>nasal</command>
      <script>controls.toggleLandingLights();</script>
    </binding>
  </button>
  <button n="15">
    <desc>Nose Steering</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/gear/nose-wheel-steering</property>
      <value>1</value>
    </binding>
    <mod-up>
      <binding>
        <command>property-assign</command>
        <property>controls/gear/nose-wheel-steering</property>
        <value>0</value>
      </binding>
    </mod-up>
  </button>
  <button n="16">
    <desc type="string">Head Track</desc>
    <binding>
      <command>property-toggle</command>
      <property>/sim/headtracker/config/enable</property>
      <value>0</value>
      <value>1</value>
    </binding>
    <repeatable type="string">false</repeatable>
  </button>
  <button n="17">
    <desc type="string">SBS</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">setprop("/sim/rendering/osg-displaysettings/stereo-mode", "HORIZONTAL_SPLIT");</script>
    </binding>
    <repeatable type="string">false</repeatable>
  </button>
  <button n="18">
    <desc type="string">Stereo Reset</desc>
    <binding>
      <command type="string">nasal</command>
      <script type="string">setprop("/sim/rendering/osg-displaysettings/stereo-mode", "OFF");</script>
    </binding>
    <repeatable type="string">false</repeatable>
  </button>
  <!--<button n="19">
    <desc>Toggle canopy</desc>
    <binding>
      <command>property-toggle</command>
      <property>/controls/flight/spoilers</property>
    </binding>
  </button>
  <button n="20">
    <desc>Start Engine</desc>
    <binding>
      <command>nasal</command>
      <script>controls.startEngine(1)</script>
    </binding>
    <mod-up>
      <binding>
        <command>nasal</command>
        <script>controls.startEngine(0)</script>
      </binding>
    </mod-up>
  </button>-->
</PropertyList>
